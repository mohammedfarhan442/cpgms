#include <stdio.h>
int
main () 
 
{
  
 
char name[50];		//character size
  
int j = 0;			//ANCII storing
  
printf ("\n -----|| Change the character into ANCII ||------\n\n");
  
 
printf ("Enter the character to convert:");
  
scanf ("%s", name);
  
 
while (name[j] != '\0')
    
 
    {
      

 
printf ("%d \t", name[j]);

printf("\nCreated by Md.Farhan\n");
      
 
j++;
    
 
}
  
 
return 0;

 
}
