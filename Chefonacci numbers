                  for(j6=j5;j6<n;j6++)

                  {

                    for(j7=j6;j7<n;j7++)

                    {

                      for(j8=j7;j8<n;j8++)

                      {

                        for(j9=j8;j9<n;j9++)

                        {

                          temp=arr[i]+arr[j]+arr[j2]+arr[j3]+arr[j4]+arr[j5]+arr[j6]+arr[j7]+arr[j8]+arr[j9];

                          if(temp==x)

                          {

                            ans++;

                          }

                          else if(temp>x)

                          {

                            break;

                          }

                        }

                        if(temp>x)

                        {

                          break;

                        }

                      }

                      if(temp>x)

                      {

                        break;

                      }

                    }

                    if(temp>x)

                    {

                      break;

                    }

                  }

                  if(temp>x)

                  {

                    break;

                  }

                }

                if(temp>x)

                {

                  break;

                }

              }

              if(temp>x)

              {

                break;

              }

            }

            if(temp>x)

            {

              break;

            }

          }

          if(temp>x)

          {

            break;

          }

        }

        if(temp>x)

        {

          break;

