#include<stdio.h>
#include<stdlib.h>
int main()

{

  int a,b,c,q,x,k,i,ans,temp,j,j2,j3,j4,j5,j6,j7,j8,j9,n;

  int arr[50];

  int flag=1;

  a=1;

  b=2;

  i=1;

  arr[0]=1;

  while(b<1000000000)

  {

    arr[i]=b;

    i++;

    c=b;

    b+=a;

    a=c;

  }

  scanf("%d",&q);

  while (q--)

  {

    scanf("%d %d",&x,&k);

    ans=0;

    n=43;

