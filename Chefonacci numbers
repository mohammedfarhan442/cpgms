    else if(k==6)

    {

      for(i=0;i<n && flag;i++)

      {

        for(j=i;j<n && flag;j++)

        {

          for(j2=j;j2<n && flag;j2++)

          {

            for(j3=j2;j3<n && flag;j3++)

            {

              for(j4=j3;j4<n && flag;j4++)

              {

                for(j5=j4;j5<n;j5++)

                {

                  temp=arr[i]+arr[j]+arr[j2]+arr[j3]+arr[j4]+arr[j5];

                  if(temp==x)

                  {

                    ans++;

                  }

                  else if(temp>x)

                  {

                    break;

