    {

      for(i=0;i<n&&flag;i++)

      {

        if(arr[i]==x)

        {

          ans=1;

          break;

        }

        else if(arr[i]>x)

        {

          break;

        }

      }

    }

    else if(k==2)

    {

      for(i=0;i<n&&flag;i++)

      {

        for(j=i;j<n&&flag;j++)

        {

          if(arr[i]+arr[j]==x)

          {

            ans++;

          }

          else if(arr[i]+arr[j]>x)

          {

            break;

          }

        }

      }

    }

    else if(k==3)

    {

      for(i=0;i<n&&flag;i++)

      {

        for(j=i;j<n && flag;j++)

        {

          for(j2=j;j2<n && flag;j2++)

          {

            temp=arr[i]+arr[j]+arr[j2];

            if(temp==x)

            {

              ans++;

            }

            else if(temp>x)

            {

              break;

            }

          }

        }

      }

    }

