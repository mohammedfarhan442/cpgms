#include<stdio.h>
#include<math.h>

 int main()
 {
 int x1, x2, y1, y2, X, Y, dist;
 printf("Enter the value of the co-odinates of first poitn:");
 scanf("%d%d", &x1, &x2);
 printf("Enter the value of the co-ordinates of second poitns:");
 scanf("%d%d", &y1, &y2);
 
X= x1 + x2;
Y= y1 + y2;
 
dist= sqrt((X*X)-(Y*Y));

printf("Distance of two points:%d \n", dist);
 return 0;
 }
